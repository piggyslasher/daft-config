{
    "presets": [
        [
            "env",
            {
                "targets": {
                    "node": "current"
                }
            }
        ]
    ],
    "plugins": [
        "transform-object-rest-spread",
        "transform-util-promisify",
        ["transform-class-properties", { "spec": true }],
        [
          "transform-es2015-modules-commonjs",
          {
            "spec": true
          }
        ]
      ],
      "sourceMaps": "inline"
    }
